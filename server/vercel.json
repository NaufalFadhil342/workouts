{
  "version": 2,
  "builds": [
    {
      "src": "./api/index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/",
      "headers": { "cache-control": "s-maxage=1000" },
      "dest": "./api/index.js"
    },
    { "src": "/(.*)", "dest": "./api/index.js" }
  ],
  "rewrites": [
    {
      "source": "/",
      "missing": [
        {
          "key": "token"
        }
      ],
      "destination": "./api/index.js"
    }
  ]
}
